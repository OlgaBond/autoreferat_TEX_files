	issn = {03003256},
	journal = {Zoologica Scripta},
	keywords = {Central Asia,phylogeography,speciation},
	mendeley-groups = {ВКР_лит},
	month = {sep},
	number = {5},
	pages = {535--548},
	title = {{Cryptic variation in mole voles Ellobius (Arvicolinae, Rodentia) of Mongolia}},
	url = {http://doi.wiley.com/10.1111/zsc.12440},
	volume = {49},
	year = {2020}
}
